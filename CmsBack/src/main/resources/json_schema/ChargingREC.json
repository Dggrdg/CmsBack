{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ChargingREC",
  "type": "object",
  "properties": {
    "ChargingREC": {
      "type": "object",
      "properties": {
        "UpdateTime": {
          "type": "string",
          "pattern": "^(?:\\d{4})-(?:0[1-9]|1[0-2])-(?:0[1-9]|[12][0-9]|3[01]) (?:[01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]$"
        },
        "ParkID": { "type": "string" },
        "RecDate": {
          "type": "string",
          "pattern": "^(?:\\d{4})-(?:0[1-9]|1[0-2])-(?:0[1-9]|[12][0-9]|3[01])"
        },
        "ChargingRecList": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "ConnectorID": { "type": "string" },
              "ChargeTime": { "type": "integer", "minimum": 0 },
              "StartTime": {
                "type": "string",
                "pattern": "^(?:\\d{4})-(?:0[1-9]|1[0-2])-(?:0[1-9]|[12][0-9]|3[01]) (?:[01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]$"
              },
              "EndTime": {
                "type": "string",
                "pattern": "^(?:\\d{4})-(?:0[1-9]|1[0-2])-(?:0[1-9]|[12][0-9]|3[01]) (?:[01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]$"
              },
              "ChargeKWH": { "type": "number", "minimum": 0 },
              "UpdateTime": {
                "type": "string",
                "pattern": "^(?:\\d{4})-(?:0[1-9]|1[0-2])-(?:0[1-9]|[12][0-9]|3[01]) (?:[01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]$"
              }
            },
            "required": [
			  "ParkID",
			  "RecDate",
              "ConnectorID",
              "ChargeTime",
              "StartTime",
              "EndTime",
              "ChargeKWH",
              "UpdateTime"
            ]
          }
        }
      },
      "required": ["UpdateTime", "ParkID", "RecDate", "ChargingRecList"]
    }
  },
  "required": ["ChargingREC"]
}
